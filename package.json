{
	"name": "cat-wiki-client",
	"description": "cat-wiki-client",
	"version": "0.0.1",
	"scripts": {
		"dev": "run-p watch:*",
		"watch:css": "postcss src/node_modules/global.pcss -o static/global.css -w",
		"watch:dev": "sapper dev",
		"build": "run-s build:css build:sapper",
		"build:css": "cross-env NODE_ENV=production postcss src/node_modules/global.pcss -o static/global.css",
		"build:sapper": "sapper build --legacy",
		"build:export": "sapper export --legacy",
		"export": "run-s build:css build:export",
		"start": "node __sapper__/build",
		"serve": "serve ___sapper__/export"
	},
	"dependencies": {
		"compression": "^1.7.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongoose": "^5.10.14",
		"node-fetch": "^2.6.1",
		"sapper-page-loading-bar": "^1.0.0",
		"sirv": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"@rollup/plugin-babel": "^5.0.0",
		"@rollup/plugin-commonjs": "^14.0.0",
		"@rollup/plugin-node-resolve": "^8.0.0",
		"@rollup/plugin-replace": "^2.2.0",
		"@rollup/plugin-url": "^5.0.0",
		"autoprefixer": "^9.0.0",
		"cross-env": "^7.0.2",
		"cssnano": "^4.1.10",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.1.1",
		"postcss-cli": "^8.2.0",
		"postcss-load-config": "^3.0.0",
		"postcss-nested": "^5.0.1",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"sapper": "^0.28.0",
		"svelte": "^3.17.3",
		"svelte-preprocess": "^4.5.2",
		"tailwindcss": "^1.9.6"
	}
}
